	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#666f78"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": [
				"comment.block.preprocessor",
				"meta.shebang",
			],
			"settings": {
				"foreground": "#005493"
			}
		},
		{
			"name": "Comments: Documentation",
